  "dependencies": {
    "bootstrap": "^5.3.1",
    "bootstrap-icons": "^1.10.5",
    "jest": "^29.7.0"
  },
  "devDependencies": {
    "eslint": "^8.52.0",
    "eslint-plugin-jest": "^27.4.3",
    "live-server": "^1.2.2",
